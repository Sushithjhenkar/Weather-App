{
    "name": "weather app",
    "version": 2,
    "builds": [
      { "src": "app/__init__.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/app/__init__.py" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "templates/index.html" }
    ]
  }
  
{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "/404.html"
      }
    ]
  }
  